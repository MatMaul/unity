{
	"ver": "2",
	"name": "sys:linkedin",
	"description": "Translation profile for LinkedIn authn",
	"type": "INPUT",
	"rules": [
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapIdentity",
				"parameters": [
					"identifier",
					"attr['id']",
					"sys:all",
					"CREATE_OR_MATCH"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"firstname",
					"/",
					"attr['first-name']",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"surname",
					"/",
					"attr['last-name']",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"address",
					"/",
					"attr['location.name']",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"countryCode",
					"/",
					"attr['location.country.code']",
					"CREATE_OR_UPDATE"
				]
			}
		}
	]
}