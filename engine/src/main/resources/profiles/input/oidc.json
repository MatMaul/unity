{
	"ver": "2",
	"name": "sys:oidc",
	"description": "OpenID connect core translation profile",
	"type": "INPUT",
	"rules": [
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapIdentity",
				"parameters": [
					"identifier",
					"attr['sub']",
					"sys:all",
					"CREATE_OR_MATCH"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"name",
					"/",
					"attr['name']",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
