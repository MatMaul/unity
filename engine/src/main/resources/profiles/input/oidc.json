					"/",
					"attr['address.street_address']",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"cityAddress",
					"/",
					"attr['address.locality']",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
