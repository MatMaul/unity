					"/",
					"attr['address.region']",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"postalCode",
					"/",
					"attr['address.postal_code']",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"country",
					"/",
					"attr['address.country']",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "!attr contains 'adrress'"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"address",
					"/",
					"attr['street_address'] + ', ' + attr['locality'] + ', ' + attr['region'] + ', ' + attr['postal_code'] + ', ' + attr['country']",
					"CREATE_OR_UPDATE"
				]
			}
		}
