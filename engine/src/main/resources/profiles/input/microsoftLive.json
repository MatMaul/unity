{
	"ver": "2",
	"name": "sys:microsoftLive",
	"description": "Translation profile for microsoft live  authn",
	"type": "INPUT",
	"rules": [
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapIdentity",
				"parameters": [
					"identifier",
					"attr['id']",
					"sys:all",
					"CREATE_OR_MATCH"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"name",
					"/",
					"attr['name']",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"firstname",
					"/",
					"attr['first_name']",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"surname",
					"/",
					"attr['last_name']",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"profileURL",
					"/",
					"attr['link']",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "attr['birth_day']  != null && attr['birth_month'] != null && attr['birth_year'] != null  "
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"birthday",
					"/",
					"attr['birth_day'] + '/' + attr['birth_month'] + '/' + attr['birth_year']  ",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"company",
					"/",
					"attr['employer']",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"email",
					"/",
					"attr['emails.account']",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "attr['phones.personal'] != null"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"telephoneNumber",
					"/",
					"attr['phones.personal']",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "attr['phones.modile'] != null"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"mobile",
					"/",
					"attr['phones.mobile']",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"locale",
					"/",
					"attr['locale']",
					"CREATE_OR_UPDATE"
				]
			}
		},
		{
			"condition": {
				"conditionValue": "true"
			},
			"action": {
				"name": "mapAttribute",
				"parameters": [
					"address",
					"/",
					"attr['adrresses.personal']",
					"CREATE_OR_UPDATE"
				]
			}
		}
	]
}